<h1>Listing logs</h1>

<table>
  <tr>
    <th>File Name</th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= link_to log.filename, log %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Destroy', log, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
